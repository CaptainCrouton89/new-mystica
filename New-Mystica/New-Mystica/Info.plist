<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>UIAppFonts</key>
	<array>
		<string>Bungee-Regular.ttf</string>
		<string>Metamorphous-Regular.ttf</string>
		<string>CinzelDecorative-Regular.ttf</string>
		<string>IMFellFrenchCanon-Regular.ttf</string>
	</array>
