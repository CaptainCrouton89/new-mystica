{
  "name": "mystica-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "supabase:types": "supabase gen types typescript --linked > src/types/database.types.ts",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.53",
    "@aws-sdk/client-s3": "^3.913.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "ai": "^5.0.76",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^9.38.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "replicate": "^1.3.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
